@using WebAppAspNetMvcImportExcel.Models;
@model List<LogHistory>

<h3>Услуги</h3>




<table class="table table-hover table-bordered" style="text-align:center">
    <thead>
        <tr class="bg-secondary text-white">
            <th style="text-align:center">
                Импорт начат в:
            </th>
            <th style="text-align:center">
                Импорт закончен в:
            </th>
            <th style="text-align:center">
                Распознанных строк
            </th>
            <th style="text-align:center">
                Не распознанных строк
            </th>
            <th style="text-align:center">
                Удалить запись импорта
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var logHistory in Model)
        {
        <tr>
            <td>
                @logHistory.StartImport
            </td>
            <td>
                @logHistory.EndImport
            </td>
            <td>
                @logHistory.SuccessCount
            </td>
            <td>
                @logHistory.FailedCount
            </td>
            <td class="grid-cell-action">
                <div class="btn-group mb-3">
                    <a href="/ImportOrders/DeleteLog/@logHistory.Id" class="btn btn-outline-secondary">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-x-square" viewBox="0 0 16 16">
                            <path d="M14 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h12zM2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2z" />
                            <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z" />
                        </svg>
                    </a>


                </div>
            </td>
        </tr>
        }
    </tbody>
</table>

